msgctxt "no_receivers"
msgid "No applications are autostarting."
msgstr "Немає програм для автозапуску."

msgctxt "no_receivers_filtered"
msgid "No applications are autostarting within your current selection."
msgstr "Жодна за програм не стартує автоматично з поточного виділення."

msgctxt "still_loading"
msgid ""
"Please wait while I am loading the applications installed on your phone..."
msgstr "Будь ласка, почекайте, поки завантажуються встановлені на Вашому телефоні програми..."

msgctxt "change_filter_settings"
msgid "Change filter settings."
msgstr "Зміна параметрів фільтрування."

msgctxt "appliation_info"
msgid "Application Info"
msgstr "Інформація про програму"

msgctxt "find_in_market"
msgid "Find in Market"
msgstr "Знайти в Market"

msgctxt "find_in_appstore"
msgid "Find in Appstore"
msgstr "Знайти в Appstore"

msgctxt "disable"
msgid "Disable"
msgstr "Відключити"

msgctxt "enable"
msgid "Enable"
msgstr "Включити"

msgctxt "view_options"
msgid "View"
msgstr "Переглянути"

msgctxt "group_by_package"
msgid "Group by application"
msgstr "Група по програмі"

msgctxt "group_by_action"
msgid "Group by event"
msgstr "Група по події"

msgctxt "hide_sys_apps"
msgid "Hide system apps"
msgstr "Приховати системні програми"

msgctxt "show_changed_only"
msgid "Show changed only"
msgstr "Показати тільки змінені"

msgctxt "hide_unknown"
msgid "Hide unknown events"
msgstr "Приховати невідомі події"

msgctxt "expand_all"
msgid "Expand all"
msgstr "Розгорнути все"

msgctxt "collapse_all"
msgid "Collapse all"
msgstr "Згорнути все"

msgctxt "reload"
msgid "Reload"
msgstr "Перезавантажити"

msgctxt "help"
msgid "Help"
msgstr "Довідка"

msgctxt "error"
msgid "Error"
msgstr "Помилка"

msgctxt "warning"
msgid "Warning"
msgstr "Попередження"

msgctxt "info"
msgid "Info"
msgstr "Інформація"

#, c-format
msgctxt "receiver_info"
msgid ""
"Receiver &lt;b&gt;%1$s&lt;/b&gt; handles action &lt;i&gt;%2$s&lt;/i&gt; with"
" priority &lt;b&gt;%3$d&lt;/b&gt;."
msgstr "Приймач &lt;b&gt;%1$s&lt;/b&gt; виконує дію &lt;i&gt;%2$s&lt;/i&gt; з пріоритетом &lt;b&gt;%3$d&lt;/b&gt;."

msgctxt "sys_disable"
msgid ""
"This is a system component. Do not make changes unless you know what you are"
" doing."
msgstr "Це системний компонент. Не робіть змін, якщо Ви знаєте, що ж Ви робите."

#. The verb (changing) should be at the front of the string
#, c-format
msgctxt "changing_state"
msgid "Changing %1$s..."
msgstr "Зміна %1$s..."

#, c-format
msgctxt "state_change_failed"
msgid ""
"Unable to change the state of %1$s (%2$s).\n"
"\n"
"Most likely, your device does not have root access, which is required."
msgstr "Неможливо змінити стан %1$s (%2$s).\n\nСкоріш за все Ваш пристрій не має root-доступу, який вимагається для цього."

msgctxt "close_help"
msgid "Close help and return to app"
msgstr "Закрити допомогу і повернутися до програми"
